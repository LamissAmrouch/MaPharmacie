<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".HomeFragment"
                                                   android:id="@+id/frameLayout">

    <androidx.constraintlayout.widget.Group android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:id="@+id/logo"
                                            app:constraint_referenced_ids="imageView3,textView6,textView5"/>

    <ImageView
            android:src="@drawable/logo13"
            android:layout_width="227dp"
            android:layout_height="195dp" android:id="@+id/imageView3"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="84dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.133" android:layout_marginTop="108dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:text="Ma"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6" android:textSize="18sp"
            android:fontFamily="@font/aclonica"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.536"
            android:layout_marginTop="124dp" app:layout_constraintTop_toTopOf="@+id/imageView3"
    />
    <TextView
            android:text="Trouver votre pharmacie dans un seul click !"
            android:layout_width="290dp"
            android:layout_height="63dp"
            android:id="@+id/textView4" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
            app:layout_constraintHorizontal_bias="0.491" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView3" android:textSize="18sp"
            android:textAlignment="center" android:textColor="#000000" android:fontFamily="@font/aclonica"/>
    <Button
            android:text="Commencer"
            android:layout_width="164dp"
            android:layout_height="58dp"
            android:id="@+id/commencer"
            android:textSize="18sp" android:textAlignment="center" android:fontFamily="@font/aclonica"
            android:layout_marginEnd="32dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="32dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            android:layout_marginBottom="264dp" app:layout_constraintBottom_toBottomOf="parent"
            android:textAllCaps="false" app:cornerRadius="20dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView4" app:layout_constraintVertical_bias="1.0"
            android:shadowRadius="@android:dimen/dialog_min_width_major"/>
    <TextView
            android:text="S'inscrire"
            android:layout_width="122dp"
            android:layout_height="35dp"
            android:id="@+id/textView2" android:textSize="18sp"
            android:fontFamily="@font/aclonica" android:textAllCaps="false" android:layout_marginBottom="32dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="60dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/commencer" app:layout_constraintVertical_bias="0.414"
            android:textColor="#000000"/>
    <TextView
            android:text="Se connecter"
            android:layout_width="131dp"
            android:layout_height="31dp"
            android:id="@+id/textView3" android:textSize="18sp"
            android:fontFamily="@font/aclonica" android:textAllCaps="false"
            app:layout_constraintStart_toEndOf="@+id/textView2" android:layout_marginStart="108dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="60dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.914" android:layout_marginBottom="32dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/commencer"
            app:layout_constraintVertical_bias="0.406" android:textColor="#000000"/>
    <TextView
            android:text="Pharmacie"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5" android:textSize="18sp"
            android:fontFamily="@font/aclonica" android:layout_marginEnd="112dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginBottom="24dp" app:layout_constraintBottom_toBottomOf="@+id/imageView3"
    />
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline" app:layout_constraintGuide_begin="20dp"
                                                android:orientation="horizontal"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline2" app:layout_constraintGuide_begin="60dp"
                                                android:orientation="vertical"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline3" app:layout_constraintGuide_begin="99dp"
                                                android:orientation="horizontal"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline4" app:layout_constraintGuide_begin="554dp"
                                                android:orientation="horizontal"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline5" app:layout_constraintGuide_begin="357dp"
                                                android:orientation="vertical"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline6" app:layout_constraintGuide_begin="589dp"
                                                android:orientation="horizontal"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline7" app:layout_constraintGuide_begin="20dp"
                                                android:orientation="horizontal"/>


</androidx.constraintlayout.widget.ConstraintLayout>