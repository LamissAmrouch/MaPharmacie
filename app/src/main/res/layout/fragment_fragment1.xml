<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".Fragment1" android:id="@+id/frameLayout3"
                                                   android:background="@drawable/back6">


    <androidx.constraintlayout.widget.Group android:layout_width="wrap_content" android:layout_height="wrap_content"
                                            android:id="@+id/group2"
                                            app:constraint_referenced_ids="imageView3,textView6,textView5"/>
    <ImageView
            android:src="@drawable/logo13"
            android:layout_width="0dp"
            android:layout_height="150dp" android:id="@+id/imageView3"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
    />
    <TextView
            android:text="Ma"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6" android:textSize="18sp"
            android:fontFamily="@font/aclonica"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.52"
            app:layout_constraintBottom_toTopOf="@+id/textView5" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/imageView3" app:layout_constraintVertical_bias="1.0"/>
    <TextView
            android:text="Pharmacie"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5" android:textSize="18sp"
            android:fontFamily="@font/aclonica"
            android:layout_marginBottom="20dp" app:layout_constraintBottom_toBottomOf="@+id/imageView3"
            app:layout_constraintStart_toStartOf="@+id/imageView3"
            app:layout_constraintEnd_toEndOf="@+id/imageView3" app:layout_constraintHorizontal_bias="0.647"/>

    <TextView
            android:text="Trouver votre pharmacie dans un seul click !"
            android:layout_width="292dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView4"
            android:textSize="18sp"
            android:textAlignment="center" android:textColor="#000000" android:fontFamily="@font/aclonica"
            android:layout_marginStart="32dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="32dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.49"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView3" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline4" app:layout_constraintVertical_bias="0.344"/>


    <TextView
            android:text="S'inscrire"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/inscrire" android:textSize="18sp"
            android:fontFamily="@font/aclonica" android:textAllCaps="false"
            android:textColor="#000000"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/guideline5"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/guideline7"
            app:layout_constraintVertical_bias="0.279" android:layout_marginStart="16dp"/>
    <TextView
            android:text="Se connecter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/connecter" android:textSize="18sp"
            android:fontFamily="@font/aclonica" android:textAllCaps="false"
            app:layout_constraintStart_toEndOf="@+id/inscrire" android:layout_marginStart="8dp"
            android:textColor="#000000"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginEnd="5dp" app:layout_constraintBaseline_toBaselineOf="@+id/inscrire"/>


    <Button
            android:text="Commencer"
            android:layout_width="154dp"
            android:layout_height="63dp"
            android:id="@+id/commencer" android:textAllCaps="false"
            app:cornerRadius="20dp" android:textSize="18sp" android:fontFamily="@font/aclonica"
            android:background="#68c49d" app:backgroundTint="#009C6A"
            android:shadowColor="@color/colorPrimaryDark"
            android:shadowRadius="2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="32dp" app:layout_constraintBottom_toTopOf="@+id/guideline7"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/guideline4"
            app:layout_constraintVertical_bias="1.0"/>


    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline3"
                                                android:orientation="horizontal"
                                                app:layout_constraintGuide_percent="0.021887826"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline" app:layout_constraintGuide_begin="20dp"
                                                android:orientation="horizontal"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline4" app:layout_constraintGuide_begin="253dp"
                                                android:orientation="horizontal"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline6" app:layout_constraintGuide_begin="20dp"
                                                android:orientation="vertical"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline22" app:layout_constraintGuide_begin="543dp"
                                                android:orientation="horizontal"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline5" app:layout_constraintGuide_begin="596dp"
                                                android:orientation="horizontal"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline7" app:layout_constraintGuide_begin="365dp"
                                                android:orientation="horizontal"/>

</androidx.constraintlayout.widget.ConstraintLayout>