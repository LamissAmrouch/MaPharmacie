<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".InscrireFragment" android:id="@+id/frameLayout2"
                                                   android:background="@drawable/back6">

    <androidx.constraintlayout.widget.Group android:layout_width="wrap_content" android:layout_height="wrap_content"
                                            android:id="@+id/group2"
                                            app:constraint_referenced_ids="imageView3,textView6,textView5"/>
    <ImageView
            android:src="@drawable/logo13"
            android:layout_width="0dp"
            android:layout_height="130dp" android:id="@+id/imageView3"
            app:layout_constraintTop_toTopOf="@+id/guideline3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
    />
    <TextView
            android:text="Ma"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6" android:textSize="18sp"
            android:fontFamily="@font/aclonica"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.52"
            app:layout_constraintBottom_toTopOf="@+id/textView5" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/imageView3" app:layout_constraintVertical_bias="1.0"/>
    <TextView
            android:text="Pharmacie"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5" android:textSize="18sp"
            android:fontFamily="@font/aclonica"
            android:layout_marginBottom="12dp" app:layout_constraintBottom_toBottomOf="@+id/imageView3"
            app:layout_constraintStart_toStartOf="@+id/imageView3"
            app:layout_constraintEnd_toEndOf="@+id/imageView3" app:layout_constraintHorizontal_bias="0.649"/>

    <EditText
            android:layout_width="241dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/nom"
            android:fontFamily="@font/aclonica" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            android:textAlignment="textStart" android:hint="Nom"
            app:layout_constraintTop_toBottomOf="@+id/imageView3"
    />
    <EditText
            android:layout_width="241dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/adresse"
            android:fontFamily="@font/aclonica" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            android:textAlignment="textStart"
            android:hint="Adresse"
            app:layout_constraintTop_toBottomOf="@+id/prenom"/>
    <EditText
            android:layout_width="241dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/prenom"
            android:fontFamily="@font/aclonica" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            android:textAlignment="textStart" android:hint="Prénom"
            app:layout_constraintTop_toBottomOf="@+id/nom"/>
    <EditText
            android:layout_width="241dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/nss"
            android:fontFamily="@font/aclonica"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:hint="NSS"
            app:layout_constraintTop_toBottomOf="@+id/adresse"/>
    <EditText
            android:layout_width="241dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/phone"
            android:fontFamily="@font/aclonica"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent" android:hint="Numéro Tél"
            app:layout_constraintTop_toBottomOf="@+id/nss"/>
    <Button
            android:text="Inscrire"
            app:cornerRadius="15dp"
            android:layout_width="116dp"
            android:layout_height="51dp"
            android:id="@+id/connecterBtn" android:textSize="18sp"
            android:textAllCaps="false" android:fontFamily="@font/aclonica" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintTop_toBottomOf="@+id/phone"/>
    <TextView
            android:text="Connecter à un compte existant"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/aclonica"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp"
            android:id="@+id/compteExistant"
            app:layout_constraintTop_toBottomOf="@+id/connecterBtn"
    />

</androidx.constraintlayout.widget.ConstraintLayout>